{
    "name": "edenai/php-sdk",
    "description": "PHP SDK for EdenAI",
    "type": "library",
    "require": {
        "pear/http_request2": "^2.5",
        "php": ">=5.3.0"
    },
    "require-dev": {
        "apigen/apigen": "^4.1",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "./../../bin/phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "EdenAI\\": "src/"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": {
            "EdenAI\\": "src/"
        },
        "classmap": ["src/"]
    },
    "license": "Apache-2.0",
    "authors": [
      {
        "name": "EdenAI",
        "email": "samy@datagenius.fr"
      }
    ],
    "minimum-stability": "stable"
}
